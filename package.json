{
  "author": "codeauthor1 <codeauthor2000@gmail.com> (https://www.twitter.com/codeathor1)",
  "name": "sassify-pro",
  "version": "1.0.0",
  "description": "SassifyPro is a powerful Sass/SCSS compiler designed to streamline your CSS development process by compiling Sass/SCSS (Syntactically Awesome Style Sheets) into efficient and browser-compatible CSS code. It provides an intuitive command-line interface and a wide range of features to enhance your productivity and maintainability.",
  "main": "dist/mjs/index.js",
  "types": "dist/types/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/commonjs/index.js",
      "import": "./dist/mjs/index.js",
      "default": "./dist/mjs/index.js"
    },
    "./package.json": "./package.json",
    "./dist/commonjs/index.js": "./dist/commonjs/index.js",
    "./dist/mjs/index.js": "./dist/mjs/index.js"
  },
  "bin": {
    " sassifyPro": "dist/mjs/lib/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "build:clean": "rimraf ./dist ",
    "build": "npm-run-all build:clean compile && node ./scripts/prepare-package-json.js",
    "compile:cjs": "tsc --project tsconfig.cjs.json",
    "compile:esm": "tsc --project tsconfig.mjs.json",
    "compile:types": "tsc --project tsconfig.types.json",
    "compile": "npm run lint && npm run compile:cjs && npm run compile:esm && npm run compile:types",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint --fix . --ext .js,.ts",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "precommit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": ["eslint"],
    "*.js": "eslint --cache --fix"
		},
  "dependencies": {
    "chalk": "^5.2.0",
    "chalk-animation": "^2.0.3",
    "cli-welcome": "^2.2.2",
    "commander": "^11.0.0",
    "figlet": "^1.6.0",
    "gradient-string": "^2.0.2",
    "inquirer": "^9.2.7",
    "nanospinner": "^1.1.0",
    "sass": "^1.63.6"
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "elint": "^2.0.1",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codeauthor1/sassify-pro.git"
  },
  "keywords": [
    "sass",
    "scss",
    "sassify",
    "sassify-pro",
    "css",
    "sassycss",
    "nodejs",
    "javascript",
    "libsass",
    "compile",
    "style",
    "express",
    "preprocess",
    "sass-compiler",
    "sass-autoprefixer",
    "codeauthor1"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Codeauthor1/sassify-pro/issues"
  },
  "homepage": "https://github.com/Codeauthor1/sassify-pro#readme",
  "directories": {
    "test": "test"
  }
}
