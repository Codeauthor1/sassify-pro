{
  "author": "codeauthor1 <codeauthor2000@gmail.com> (https://www.twitter.com/codeathor1)",
  "name": "sassifypro",
  "version": "1.0.0",
  "description": "SassifyPro is a powerful Sass/SCSS compiler designed to streamline your CSS development process by compiling Sass/SCSS (Syntactically Awesome Style Sheets) into efficient and browser-compatible CSS code. It provides an intuitive command-line interface and a wide range of features to enhance your productivity and maintainability.",
  "main": "dist/mjs/index.js",
  "types": "dist/types/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/commonjs/index.cjs",
      "import": "./dist/mjs/index.js",
      "default": "./dist/mjs/index.js"
    },
    "./package.json": "./package.json",
    "./dist/commonjs/*": "./dist/commonjs/*",
    "./dist/mjs/*": "./dist/mjs/*"
  },
  "imports": {},
  "bin": {
    "sassifyPro": {
      "esm": "dist/mjs/index.js",
      "cjs": "dist/commonjs/index.cjs"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "build:clean": "rimraf ./dist ",
    "build": "npm-run-all build:clean compile && node ./scripts/prepare-package-json.js",
    "compile:cjs": "tsc --project tsconfig.cjs.json && babel lib --out-dir dist",
    "compile:esm": "tsc --project tsconfig.mjs.json",
    "compile:types": "tsc --project tsconfig.types.json",
    "compile": "npm run lint && npm run compile:cjs && npm run compile:esm && npm run compile:types",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint --fix . --ext .js,.ts",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "precommit": "lint-staged",
    "dev": "npm run compile && npx yalc publish",
    "typedoc": "rimraf  ./docs && typedoc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "sass": "^1.63.6"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.27.0",
    "@types/cli-welcome": "^2.2.0",
    "@types/figlet": "^1.5.6",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "figlet": "^1.6.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nanospinner": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "websocket": "^1.0.34"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codeauthor1/sassify-pro.git"
  },
  "keywords": [
    "sass",
    "scss",
    "sassify",
    "sassifypro",
    "css",
    "sassycss",
    "nodejs",
    "javascript",
    "libsass",
    "compile",
    "style",
    "express",
    "preprocess",
    "sass-compiler",
    "sass-autoprefixer",
    "codeauthor1"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Codeauthor1/sassify-pro/issues"
  },
  "homepage": "https://github.com/Codeauthor1/sassify-pro#readme",
  "directories": {
    "test": "test"
  }
}
