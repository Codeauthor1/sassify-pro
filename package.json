{
  "author": "codeauthor1 <codeauthor2000@gmail.com> (https://www.twitter.com/codeathor1)",
  "name": "sassify-pro",
  "version": "1.0.4",
  "description": "SassifyPro is a powerful Sass compiler designed to revolutionize your web development workflow.",
  "main": "dist/index.js",
  "types": "dist/types/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./dist/index.cjs": "./dist/index.cjs",
    "./dist/index.js": "./dist/index.js"
  },
  "bin": {
    "sassify-pro": "dist/index.cjs"
  },
  "scripts": {
    "prepare": "husky install",
    "build:clean": "rimraf ./dist ",
    "build:rollup": "rollup -c rollup.config.js",
    "compile:types": "tsc --project tsconfig.types.json",
    "compile": "npm run lint && npm run build:rollup && npm run compile:types",
    "build": "npm run  build:clean && npm run compile",
    "start": "npx ts-node ./lib/index.ts",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint --fix . --ext .js,.ts",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "precommit": "lint-staged",
    "dev": "npm run compile && npx yalc publish",
    "typedoc": "rimraf  ./docs && typedoc",
    "grant:linux": "chmod +x dist/index.cjs && chmod +x dist/index.js",
    "release": "npm run build && publish.bat"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "colorette": "^2.0.20",
    "figlet": "^1.6.0",
    "glob": "^10.3.3",
    "nanospinner": "^1.1.0",
    "sass": "^1.63.6",
    "semver": "^7.5.4",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.27.0",
    "@types/figlet": "^1.5.6",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-typescript2": "^0.35.0",
    "ts-jest": "^29.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codeauthor1/sassify-pro.git"
  },
  "keywords": [
    "sass",
    "scss",
    "sassify",
    "sassifypro",
    "css",
    "sassycss",
    "libsass",
    "compile",
    "style",
    "preprocess",
    "sass-compiler",
    "autoprefixer",
    "codeauthor1"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Codeauthor1/sassify-pro/issues"
  },
  "homepage": "https://github.com/Codeauthor1/sassify-pro#readme",
  "directories": {
    "test": "test"
  }
}
